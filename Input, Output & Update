import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int marks[] = new int[50];
		Scanner sc=new Scanner(System.in);
		marks[0]=sc.nextInt();
		marks[1]=sc.nextInt();
		System.out.println(marks[0]);
		System.out.println(marks[1]);
		marks[1]=marks[1]+1;
		System.out.println(marks[1]);
		//length of Array
		System.out.println(marks.length);
	}
}
